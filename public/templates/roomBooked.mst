<div class="roombox room-busy" onclick="selectRoom({{room.id}})"> 
    <div class="row roombox-info-container">
        <div class="roombox-people-list">
            <ul>
                {{#room.employees}}
                <li>{{name}}</li>
                {{/room.employees}}
            </ul>
        </div>
        <div class="roombox-time">
            Booked Until: {{room.prettyTime}}
        </div>
        <h1 class="name">{{room.name}}</h1>
    </div>
</div>